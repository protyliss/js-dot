{
  "name": "@js-dot/workspace",
  "version": "24.137.0",
  "description": "Dot of Javascript Library based Typescript to make Line",
  "type": "module",
  "workspaces": [
    "src/js-dot",
    "src/node-dot"
  ],
  "scripts": {
    "dev:schedule": "ts-node-dev -r tsconfig-paths/register src/js-dot/schedule/set-schedule.ts",
    "dev:term": "ts-node-dev -r tsconfig-paths/register src/js-dot/schedule/term.ts",
    "dev:http": "ts-node-dev -r tsconfig-paths/register src/dev/http.ts",
    "dev:copy-properties": "ts-node-dev -r tsconfig-paths/register src/js-dot/core/functions/objects/copy-properties.ts",
    "dev:change-log": "ts-node-dev -r tsconfig-paths/register src/dev/change-log.ts",
    "dev:ts": "ts-node-dev -r tsconfig-paths/register src/dev/ts.ts",
    "dev:thread": "ts-node-dev -r tsconfig-paths/register src/dev/thread.ts",
    "dev:dynamic": "ts-node-dev -r tsconfig-paths/register src/dev/dynamic.ts",
    "dev:test": "ts-node-dev -r tsconfig-paths/register src/dev/test.ts",
    "dev:rn": "ts-node-dev -r tsconfig-paths/register src/dev/relate-nest.ts",
    "dev:api": "ts-node-dev -r tsconfig-paths/register src/dev/api.ts",
    "dev:frame": "ts-node-dev -r tsconfig-paths/register src/dev/frame.ts",
    "dev:templating": "ts-node-dev -r tsconfig-paths/register src/dev/templating.ts",
    "dev:units": "ts-node-dev -r tsconfig-paths/register src/dev/period-units.ts",
    "dev:collector": "ts-node-dev -r tsconfig-paths/register src/js-dot/frame/patterns/collector.ts",
    "dev:reliable": "ts-node-dev -r tsconfig-paths/register src/js-dot/frame/decorators/reliable.ts",
    "dev:shapr-brace": "ts-node-dev -r tsconfig-paths/register src/js-dot/frame/template/sharp-brace-template.ts",
    "dev:data": "ts-node-dev -r tsconfig-paths/register src/js-dot/data/frame/transpose.ts",
    "dev:core": "ts-node-dev -r tsconfig-paths/register src/js-dot/core/classes/abort-promise.ts",
    "dev:only-one": "ts-node-dev -r tsconfig-paths/register src/js-dot/core/decorators/only-one.ts",
    "dev:contract": "ts-node-dev -r tsconfig-paths/register src/dev/contract.ts",
    "dev:caster": "ts-node-dev -r tsconfig-paths/register src/dev/caster.ts",
    "dev:array-iter": "ts-node-dev -r tsconfig-paths/register src/js-dot/core/classes/array-iterator-base.ts",
    "dev:store": "ts-node-dev -r tsconfig-paths/register pending/store/idb/http-response.ts",
    "dev:glob": "ts-node-dev -r tsconfig-paths/register src/node-dot/core/functions/fs/glob.test.ts",
    "package-version-update": "node-liss nod e package-version",
    "test": "jest",
    "bundle": "ts-node-dev -r tsconfig-paths/register build.ts",
    "js:dev:test": "jest src/js-dot/dev",
    "js:dev:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/dev",
    "js:dev:publish": "cd dist/js-dot/dev && npm publish",
    "js:core:test": "jest src/js-dot/core",
    "js:core:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/core",
    "js:core:publish": "cd dist/js-dot/core && npm publish",
    "js:core:link": "cd dist/js-dot/core && npm link",
    "js:data:test": "jest src/js-dot/data",
    "js:data:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/data",
    "js:data:publish": "cd dist/js-dot/data && npm publish",
    "js:dynamic:test": "jest src/js-dot/dynamic",
    "js:dynamic:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/dynamic",
    "js:dynamic:publish": "cd dist/js-dot/dynamic && npm publish",
    "js:api:test": "jest src/js-dot/api",
    "js:api:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/api",
    "js:api:publish": "cd dist/js-dot/api && npm publish",
    "js:api:link": "cd dist/js-dot/api && npm link",
    "js:ui:test": "jest src/js-dot/ui",
    "js:ui:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/ui",
    "js:ui:publish": "cd dist/js-dot/ui && npm publish",
    "js:ui:link": "cd dist/js-dot/ui && npm link",
    "js:form:test": "jest src/js-dot/form",
    "js:form:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/form",
    "js:form:publish": "cd dist/js-dot/form && npm publish",
    "js:form:link": "cd dist/js-dot/form && npm link",
    "js:text:test": "jest src/js-dot/text",
    "js:text:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/text",
    "js:text:publish": "cd dist/js-dot/text && npm publish",
    "js:text:link": "cd dist/js-dot/text && npm link",
    "js:frame:test": "jest src/js-dot/frame",
    "js:frame:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/frame",
    "js:frame:publish": "cd dist/js-dot/frame && npm publish",
    "js:frame:link": "cd dist/js-dot/frame && npm link",
    "js:rx:test": "jest src/js-dot/rx",
    "js:rx:bundle": "ts-node-dev -r tsconfig-paths/register build.ts js-dot/rx",
    "js:rx:publish": "cd dist/js-dot/rx && npm publish",
    "js:rx:link": "cd dist/js-dot/rx && npm link",
    "node:builds": "ts-node-dev -r tsconfig-paths/register build.ts node-dot/core node-dot/build node-dot/build-es",
    "node:dev:test": "jest src/node-dot/dev",
    "node:dev:bundle": "ts-node-dev -r tsconfig-paths/register build.ts node-dot/dev",
    "node:dev:publish": "cd dist/node-dot/dev && npm publish",
    "node:dev:link": "cd dist/node-dot/dev && npm link",
    "node:core:test": "jest src/node-dot/core",
    "node:core:bundle": "ts-node-dev -r tsconfig-paths/register build.ts node-dot/core",
    "node:core:publish": "cd dist/node-dot/core && npm publish",
    "node:core:link": "cd dist/node-dot/core && npm link",
    "node:log:test": "jest src/node-dot/log",
    "node:log:bundle": "ts-node-dev -r tsconfig-paths/register build.ts node-dot/log",
    "node:log:publish": "cd dist/node-dot/log && npm publish",
    "node:log:link": "cd dist/node-dot/log && npm link",
    "node:build:bundle": "ts-node-dev -r tsconfig-paths/register build.ts node-dot/build",
    "node:build:publish": "cd dist/node-dot/build && npm publish",
    "node:build-es:bundle": "ts-node-dev -r tsconfig-paths/register build.ts node-dot/build-es",
    "node:build-es:publish": "cd dist/node-dot/build-es && npm publish",
    "node:work:bundle": "ts-node-dev -r tsconfig-paths/register build.ts node-dot/work",
    "node:work:publish": "cd dist/node-dot/work && npm publish"
  },
  "author": "protyliss@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@angular-devkit/core": "^15.1.6",
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.20.13",
    "@node-dot/core": "^23.38.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@types/jest": "^29.4.0",
    "esbuild": "^0.19.12",
    "esbuild-node-externals": "^1.12.0",
    "esbuild-plugin-d.ts": "^1.3.0-canary.1",
    "fake-indexeddb": "^4.0.1",
    "glob": "^8.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-change": "^1.0.2",
    "gulp-debug": "^4.0.0",
    "gulp-filter": "^7.0.0",
    "gulp-map": "0.0.2",
    "gulp-rollup": "^2.17.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-typescript-babel": "^1.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^28.1.1",
    "jest-fetch-mock": "^3.0.3",
    "pump": "^3.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.3.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.1",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.3.3"
  },
  "jest": {
    "transform": {
      "\\.tsx?$": [
        "ts-jest",
        {
          "diagnostics": true,
          "autoMapModuleNames": true
        }
      ]
    },
    "testRegex": "\\.jest\\.ts$",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "mts",
      "cts",
      "js",
      "mjs",
      "cjs"
    ],
    "verbose": false,
    "silent": false,
    "moduleNameMapper": {
      "@js-dot/(.*)": "<rootDir>/src/js-dot/$1"
    }
  },
  "dependencies": {
    "@angular-devkit/schematics": "^15.1.6",
    "@mxssfd/typedoc-theme": "^1.1.3",
    "rxjs": "^7.5.6",
    "typedoc-material-theme": "^1.0.2"
  }
}
