{
  "name"   : "@js-dot/workspace",
  "type": "module",
  "scripts": {
    "doc": "typedoc",
    "core:jest": "jest src/js-dot/core",
    "core:bundle": "ts-node-dev -r tsconfig-paths/register ../../build.ts js-dot/core",
    "core:publish": "cd dist/js-dot/core && npm publish",
    "core:link": "cd dist/js-dot/core && npm link",
    "data:test": "jest src/js-dot/data",
    "data:bundle": "ts-node-dev -r tsconfig-paths/register ../../build.ts js-dot/data",
    "data:publish": "cd dist/js-dot/data && npm publish",
    "dynamic:test": "jest src/js-dot/dynamic",
    "dynamic:bundle": "ts-node-dev -r tsconfig-paths/register ../../build.ts js-dot/dynamic",
    "dynamic:publish": "cd dist/js-dot/dynamic && npm publish",
    "api:test": "jest src/js-dot/api",
    "api:bundle": "ts-node-dev -r tsconfig-paths/register ../../build.ts js-dot/api",
    "api:publish": "cd dist/js-dot/api && npm publish",
    "api:link": "cd dist/js-dot/api && npm link",
    "ui:test": "jest src/js-dot/ui",
    "ui:bundle": "ts-node-dev -r tsconfig-paths/register ../../build.ts js-dot/ui",
    "ui:publish": "cd dist/js-dot/ui && npm publish",
    "ui:link": "cd dist/js-dot/ui && npm link",
    "form:test": "jest src/js-dot/form",
    "form:bundle": "ts-node-dev -r tsconfig-paths/register ../../build.ts js-dot/form",
    "form:publish": "cd dist/js-dot/form && npm publish",
    "form:link": "cd dist/js-dot/form && npm link",
    "text:test": "jest src/js-dot/text",
    "text:bundle": "ts-node-dev -r tsconfig-paths/register ../../build.ts js-dot/text",
    "text:publish": "cd dist/js-dot/text && npm publish",
    "text:link": "cd dist/js-dot/text && npm link",
    "frame:test": "jest src/js-dot/frame",
    "frame:bundle": "ts-node-dev -r tsconfig-paths/register ../../build.ts js-dot/frame",
    "frame:publish": "cd dist/js-dot/frame && npm publish",
    "frame:link": "cd dist/js-dot/frame && npm link",
    "rx:test": "jest src/js-dot/rx",
    "rx:bundle": "ts-node-dev -r tsconfig-paths/register ../../build.ts js-dot/rx",
    "rx:publish": "cd dist/js-dot/rx && npm publish",
    "rx:link": "cd dist/js-dot/rx && npm link"
  }
}
